

var pilots = [
{
id: 10,
name: "Poe Dameron",
years: 14,
},
{
id: 2,
name: "Temmin 'Snap' Wexley",
years: 30,
},
{
id: 41,
name: "Tallissan Lintra",
years: 16,
},
{
id: 99,
name: "Ello Asty",
years: 22,
}
];

var TotalYears = pilots.reduce (function (ageSum, pilot) { return ageSum + pilot.years; }, 0);

console.log('The Total Years :' + TotalYears);




